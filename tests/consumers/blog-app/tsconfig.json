{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"pounce-board/*": [
				"../../../dist/lib/*"
			],
			"pounce-board/adapters/*": [
				"../../../dist/adapters/*"
			]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx"
	]
}