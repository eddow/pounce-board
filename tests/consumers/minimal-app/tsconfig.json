{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noEmit": true,
		"jsx": "react-jsx",
		"jsxImportSource": "pounce-ts",
		"baseUrl": ".",
		"paths": {
			"pounce-ts": [
				"../../../../pounce-ts/src/lib"
			],
			"pounce-ts/*": [
				"../../../../pounce-ts/src/lib/*"
			],
			"mutts": [
				"../../../../mutts/src"
			],
			"mutts/*": [
				"../../../../mutts/src/*"
			],
			"pounce-board": [
				"../../../../pounce-board/src/server/index.ts"
			],
			"pounce-board/client": [
				"../../../../pounce-board/src/client/index.ts"
			],
			"pounce-board/server": [
				"../../../../pounce-board/src/server/index.ts"
			]
		}
	},
	"include": [
		"**/*",
		"../../../../pounce-ts/**/*",
		"../../../../mutts/**/*"
	]
}